{
  "name": "Bechdel_Test",
  "version": "0.3.0",
  "engines": {
    "node": "8.8.0"
  },
  "description": "A node based interactive site to analyze a movie script in order to tell if it passes the Bechdel Test.",
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoeKarlsson/bechdel-test"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "react",
    "bechdel",
    "bechdel Test",
    "Feminism",
    "Feminist",
    "Film",
    "movie",
    "script",
    "parser",
    "movie script",
    "feminist theory"
  ],
  "author": "Joe Karlsson & Laurel Karlsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoeKarlsson/bechdel-test/issues"
  },
  "homepage": "https://github.com/JoeKarlsson/bechdel-test",
  "scripts": {
    "start": "node src/server/server.js",
    "test": "jest --coverage",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "test:watch": "jest --watch",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:coverage": "jest --coverage && open ./coverage/index.html",
    "lint": "eslint src && npm run lint:css",
    "lint:fix": "npm run lint -- --fix",
    "lint:out": "npm run lint -- -f checkstyle -o coverage/checkstyle.xml",
    "lint:css": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:css:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "healthcheck": "npm run lint && npm run lint:css && npm run test:coverage",
    "build": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.config.prod.js --progress --profile --colors",
    "postinstall": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.config.prod.js --progress --profile --colors",
    "runProd": "export NODE_ENV=production MONGODB_URI=mongodb://localhost/bechdelTest PORT=8081 MYAPIFILMS=d44147a7-5e6e-4450-92ba-773be44791ce THEMOVIEDB=6ec9ddad40a319b47c562e0838f7eda3 && node src/server/server.js"
  },
  "dependencies": {
    "autoprefixer": "7.2.x",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.x",
    "babel-register": "6.x.x",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "0.28.x",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "4.x.x",
    "eslint-config-airbnb": "16.1.x",
    "eslint-plugin-import": "2.8.x",
    "eslint-plugin-jsx-a11y": "6.0.x",
    "eslint-plugin-react": "7.4.x",
    "express": "^4.13.3",
    "express-partials": "^0.3.0",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "3.x.x",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "2.x.x",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.8.1",
    "jest": "^21.2.1",
    "jquery": "^2.2.3",
    "jscs": "^2.1.1",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.3",
    "method-override": "^2.3.5",
    "mongoose": "^4.13.7",
    "multer": "^1.1.0",
    "nock": "^9.1.4",
    "node-sass": "^3.7.0",
    "npm": "^3.9.5",
    "nyc": "^11.3.0",
    "postcss-loader": "^0.6.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "request": "^2.67.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "sinon-mongoose": "^1.2.1",
    "stats-webpack-plugin": "0.6.x",
    "style-loader": "^0.12.4",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-scss": "^2.2.0",
    "stylelint-webpack-plugin": "0.9.x",
    "supertest": "^1.2.0",
    "url-loader": "^0.6.2",
    "util": "^0.10.3",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "1.x.x",
    "webpack-dev-server": "2.9.x",
    "webpack-hot-middleware": "2.9.x"
  },
  "devDependencies": {}
}
