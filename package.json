{
	"name": "Bechdel_Test",
	"version": "1.0.1",
	"engines": {
		"node": "12.9.1"
	},
	"description": "A node based interactive site to analyze a movie script in order to tell if it passes the Bechdel Test.",
	"main": "server/server.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/JoeKarlsson/bechdel-test"
	},
	"keywords": [
		"express",
		"webpack",
		"node",
		"react",
		"bechdel",
		"bechdel Test",
		"Feminism",
		"Feminist",
		"Film",
		"movie",
		"script",
		"parser",
		"movie script",
		"feminist theory"
	],
	"author": "Joe Karlsson & Laurel Karlsson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/JoeKarlsson/bechdel-test/issues"
	},
	"homepage": "https://github.com/JoeKarlsson/bechdel-test",
	"scripts": {
		"start": "set NODE_ENV=development && node src/server/server.js",
		"start:dev": "set NODE_ENV=development && nodemon src/server/server.js",
		"stop": "killall -s KILL node",
		"test": "jest --coverage",
		"coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
		"test:watch": "jest --watch",
		"test:updateSnapshot": "jest --updateSnapshot",
		"test:coverage": "jest --coverage",
		"lint": "eslint src && npm run lint:css && npm run lint:md",
		"lint:fix": "npm run lint -- --fix",
		"lint:out": "npm run lint -- -f checkstyle -o coverage/checkstyle.xml",
		"lint:css": "stylelint 'src/**/*.scss' --syntax scss",
		"lint:css:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
		"lint:md": "remark .",
		"healthcheck": "npm run lint && npm run test:coverage",
		"processScript": "nodemon src/server/helper/processScript.js",
		"build": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.config.prod.js --progress --profile --colors",
		"postinstall": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.config.prod.js --progress --profile --colors",
		"runProd": "export NODE_ENV=production MONGODB_URI=mongodb://localhost/bechdelTest PORT=8081 MYAPIFILMS=d44147a7-5e6e-4450-92ba-773be44791ce THEMOVIEDB=6ec9ddad40a319b47c562e0838f7eda3 && node src/server/server.js",
		"snyk-protect": "snyk protect",
		"prepublish": "npm run snyk-protect"
	},
	"dependencies": {
		"@babel/core": "^7.8.4",
		"@babel/plugin-transform-react-jsx": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"@testing-library/react": "^9.4.0",
		"@uppy/core": "^1.7.1",
		"@uppy/dashboard": "^1.5.2",
		"@uppy/react": "^1.4.2",
		"@uppy/xhr-upload": "^1.4.2",
		"autoprefixer": "7.2.5",
		"babel-jest": "^25.1.0",
		"babel-loader": "^8.0.6",
		"body-parser": "1.18.2",
		"connect-history-api-fallback": "1.5.0",
		"coveralls": "^3.0.9",
		"cross-env": "5.1.3",
		"css-loader": "^3.4.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"es6-promise": "4.2.1",
		"eslint": "4.18.2",
		"eslint-config-airbnb": "16.1.0",
		"eslint-config-prettier": "2.9.0",
		"eslint-plugin-import": "2.8.0",
		"eslint-plugin-jsx-a11y": "6.0.3",
		"eslint-plugin-prettier": "^2.4.0",
		"eslint-plugin-react": "7.5.1",
		"express": "4.16.0",
		"express-partials": "^0.3.0",
		"fetch-mock": "5.13.1",
		"file-loader": "^5.0.2",
		"helmet": "^3.21.2",
		"history": "^4.10.1",
		"html-webpack-plugin": "^3.2.0",
		"identity-obj-proxy": "3.0.0",
		"isomorphic-fetch": "2.2.1",
		"jest": "^25.1.0",
		"jest-fetch-mock": "^3.0.1",
		"json-loader": "0.5.7",
		"method-override": "2.3.10",
		"mini-css-extract-plugin": "^0.9.0",
		"mockingoose": "^2.13.2",
		"mongoose": "^5.8.11",
		"multer": "^1.3.0",
		"nock": "9.1.6",
		"node-sass": "^4.13.1",
		"nyc": "^15.0.0",
		"postcss-loader": "2.1.0",
		"prettier": "^1.19.1",
		"prettier-stylelint": "^0.4.2",
		"prop-types": "15.6.0",
		"react": "^16.12.0",
		"react-addons-test-utils": "^15.6.2",
		"react-dom": "^16.12.0",
		"react-router": "^5.1.2",
		"react-router-dom": "^5.1.2",
		"react-test-renderer": "^16.2.0",
		"recharts": "^1.0.0-beta.9",
		"remark-cli": "^7.0.1",
		"remark-lint": "6.0.1",
		"remark-preset-lint-recommended": "3.0.1",
		"rimraf": "2.4.3",
		"sass-loader": "^8.0.2",
		"snyk": "^1.290.1",
		"stats-webpack-plugin": "0.6.2",
		"style-loader": "^1.1.3",
		"stylelint": "^13.0.0",
		"stylelint-config-recommended-scss": "^4.2.0",
		"stylelint-scss": "^3.14.2",
		"stylelint-webpack-plugin": "^1.2.1",
		"supertest": "3.0.0",
		"url-loader": "0.6.2",
		"util": "0.10.3",
		"webpack": "^4.41.5",
		"webpack-dev-middleware": "2.0.4",
		"webpack-dev-server": ">=3.1.11",
		"webpack-hot-middleware": "2.21.0"
	},
	"devDependencies": {
		"pretty-quick": "^1.3.0",
		"webpack-cli": "^3.3.10"
	},
	"optionalDependencies": {
		"husky": "^0.14.3"
	},
	"snyk": true
}
