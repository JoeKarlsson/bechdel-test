{
  "name": "Bechdel_Test",
  "version": "0.3.0",
  "engines": {
    "node": "5.8.0"
  },
  "description": "A node based interactive site to analyze a movie script in order to tell if it passes the Bechdel Test.",
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JoeKarlsson1/bechdel-test"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "react",
    "bechdel",
    "bechdel Test",
    "Feminism",
    "Feminist",
    "Film",
    "movie",
    "script",
    "parser",
    "movie script",
    "femist theory"
  ],
  "author": "Joe Carlson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoeKarlsson1/bechdel-test/issues"
  },
  "homepage": "https://github.com/JoeKarlsson1/bechdel-test",
  "scripts": {
    "test": "./node_modules/.bin/mocha -R spec tests/server",
    "test:server:watch": "npm run test --watch",
    "test:client": "mocha tests/app/.setup.js tests/app/**/*-test.js",
    "start": "nodemon server/server.js",
    "build": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "eslint": "./node_modules/.bin/eslint .",
    "postinstall": "rimraf dist && set NODE_ENV=production && webpack --config ./webpack.production.config.js --progress --profile --colors"
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "babel-cli": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.1",
    "css-loader": "^0.19.0",
    "eslint": "^2.9.0",
    "eslint-loader": "^1.3.0",
    "express": "^4.13.3",
    "express-favicon": "^1.0.1",
    "express-partials": "^0.3.0",
    "express-session": "^1.13.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "jquery": "^2.2.3",
    "json-loader": "^0.5.3",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.13",
    "multer": "^1.1.0",
    "node-sass": "^3.7.0",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "postcss-loader": "^0.6.0",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^2.4.0",
    "react-transform-hmr": "^1.0.0",
    "request": "^2.67.0",
    "sass-loader": "^3.2.0",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "util": "^0.10.3",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "jscs": "^2.1.1",
    "jsdom": "^6.5.1",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "mocha-loader": "^0.7.1",
    "nodemon": "^1.9.2",
    "react-addons-test-utils": "^0.14.8",
    "rimraf": "^2.4.3",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.2.0",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0",
    "webpack-node-externals": "^1.2.0"
  }
}
